{
  "name": "lending_telecard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "lint:css": "./node_modules/stylelint/bin/stylelint.js './src/**/*.ts'",
    "tslint": "./node_modules/tslint/bin/tslint -p tsconfig.json './src/**/*.{d.ts,ts,tsx}'",
    "tslint:fix": "./node_modules/tslint/bin/tslint -p tsconfig.json --fix './src/**/*.{d.ts,ts,tsx}'",
    "prettier:base": "prettier --config ./prettier.config.js",
    "prettier:check": "npm run prettier:base -- --list-different './src/**/*.{d.ts,ts,tsx}'",
    "prettier:write": "npm run prettier:base -- --write './src/**/*.{d.ts,ts,tsx}'",
    "lint": "npm run tslint && npm run prettier:check && npm run lint:css",
    "precommit": "npm run lint && npm run prettier:write"
  },
  "pre-commit": [
    "tslint",
    "prettier:write",
    "lint:css"
  ],
  "dependencies": {
    "@holism/core": "^1.1.1",
    "@holism/dotfiles": "0.0.11",
    "axios": "^0.18.0",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "mobile-detect": "^1.4.3",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "mobx-react-router": "^4.0.7",
    "mobx-utils": "^5.1.0",
    "moment": "^2.23.0",
    "polished": "3.0.0",
    "qs": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^4.2.0",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/qs": "^6.5.1",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.4",
    "prettier": "^1.17.1",
    "react-app-rewired": "^2.1.3",
    "stylelint": "^10.0.1",
    "tslint": "^5.16.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
